#include <stdio.h>

void  telaSobre ( void );
void  telaPrincipal ( void );

int main(void) {
  telaSobre ();
  telaPrincipal();
  return 0;
}

void  telaSobre ( void ) {
    printf ( " \n " );
    printf ( " /////////////////////////////////////////////////////////\n " );
    printf ( " /// Projeto de controle de despensa doméstica /// \n " );
    printf ( " /////////////////////////////////////////////////////////\n " );
    printf ( " /// Auxilia no controle dos gastos /// \n " );
    printf ( " /// Auxilia na organização doméstica /// \n " );
    printf ( " /// Evita gastos desnecessários /// \n " );
    printf ( " /////////////////////////////////////////////////////////\n " );
}

void  telaPrincipal ( void ) {
    printf ( " \n " );
    printf ( " /////////////////////////////////////////////////////////\n " );
    printf ( " /// 1. Módulo Cliente /// \n " );
    printf ( " /// 2. Módulo Renda /// \n " );
    printf ( " /// 3. Módulo Controle dos gastos /// \n " );
    printf ( " /////////////////////////////////////////////////////////\n " );

}
